
public class Pattern2 {

	public static void main(String[] args) 
	{
		int p=5;
		for(int row=1;row<=5;row++)
		{
			for(int col=5;col>=1;col--)
			{
				
				for(int k=1;k==col;k++)
				{
					if(col==k)
					{
						System.out.print("*");
					}
					else
					{
						System.out.print(" ");
					}
				}
				
			}
			
			System.out.println();

			
		}

	}

}
