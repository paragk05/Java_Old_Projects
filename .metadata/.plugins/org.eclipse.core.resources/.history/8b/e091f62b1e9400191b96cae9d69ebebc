
public class Pattern2 {

	public static void main(String[] args) 
	{
		
		for(int row=1;row<=5;row++)
		{
			for(int col=1;col<=;col++)
			{
				System.out.print(" ");
			}	
			for(int star=1;star<=5;star++)
			{
				System.out.print("*");
			}
				
			
			
			System.out.println();

			
		}

	}

}
